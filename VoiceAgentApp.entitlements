<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Voice over IP (VoIP) Push Notifications -->
	<key>com.apple.developer.pushkit</key>
	<true/>

	<!-- Background Modes -->
	<key>com.apple.developer.background-modes</key>
	<array>
		<string>audio</string>
		<string>voip</string>
	</array>

	<!-- Network Extensions (if using Tailscale SDK) -->
	<key>com.apple.developer.networking.networkextension</key>
	<array>
		<string>packet-tunnel-provider</string>
	</array>

	<!-- App Groups (for sharing data between app and extensions) -->
	<key>com.apple.security.application-groups</key>
	<array>
		<string>group.com.yourcompany.voiceagent</string>
	</array>
</dict>
</plist>
